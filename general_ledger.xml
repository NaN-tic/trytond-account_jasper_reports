<tryton>
    <data>
        <record model="ir.ui.view" id="print_general_ledger_start_view_form">
            <field name="model">account_jasper_reports.print_general_ledger.start</field>
            <field name="type">form</field>
            <field name="name">print_general_ledger_start_form</field>
        </record>
        <record model="ir.action.wizard" id="wizard_print_general_ledger">
            <field name="name">Print General Ledger</field>
            <field name="wiz_name">account_jasper_reports.print_general_ledger</field>
        </record>
        <record model="ir.action.report" id="report_general_ledger">
            <field name="name">General Ledger</field>
            <!--<field name="model">account.move.line</field>-->
            <field name="report_name">account_jasper_reports.general_ledger</field>
            <field name="report">account_jasper_reports/general_ledger.jrxml</field>
            <field name="extension">pdf</field>
        </record>

        <menuitem id="account.menu_general_ledger"
            action="wizard_print_general_ledger" icon="tryton-print"/>
    </data>
</tryton>
